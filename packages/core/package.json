{
  "name": "@campkit/core",
  "version": "0.0.8",
  "publishConfig": {
    "access": "public"
  },
  "author": "Roger Rodriguez <roger@rrod.co>",
  "description": "Build serverless Node.js microservices fast",
  "license": "MIT",
  "homepage": "https://github.com/getcampsite/campkit/tree/master/packages/core",
  "repository": {
    "type": "git",
    "url": "https://github.com/getcampsite/campkit.git"
  },
  "keywords": [
    "microservice",
    "serverless",
    "node microservice",
    "campkit"
  ],
  "bugs": {
    "url": "https://github.com/getcampsite/campkit/issues"
  },
  "main": "dist/index.js",
  "module": "dist/campkit-core.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run-script build",
    "start": "tsdx watch",
    "build": "tsdx build --target node --format cjs",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "_publish": "npm publish --access public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@middy/core": "1.0.0-alpha.47",
    "@middy/http-json-body-parser": "1.0.0-alpha.47"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.33",
    "@types/jest": "^24.0.18",
    "husky": "^3.0.8",
    "reflect-metadata": "0.1.13",
    "tsdx": "^0.9.3",
    "tslib": "^1.10.0",
    "typescript": "^3.6.3"
  }
}
